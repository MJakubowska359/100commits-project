{
  "name": "100commits-project",
  "version": "1.0.0",
  "description": "automation tests in playwright",
  "main": "index.js",
  "scripts": {
    "format": "npx prettier --write .",
    "format:check": "npx prettier . --check \"!**.ts\"",
    "lint": "npx eslint . --ext .ts --max-warnings=0",
    "test": "npx playwright test",
    "test:headed": "npm run test -- --headed",
    "test:ui": "npm run test -- --ui",
    "show-report": "npx playwright show-report",
    "prepare": "husky"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/MJakubowska359/100commits-project.git"
  },
  "author": "Monika Jakubowska",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/MJakubowska359/100commits-project/issues"
  },
  "homepage": "https://github.com/MJakubowska359/100commits-project#readme",
  "devDependencies": {
    "@faker-js/faker": "^8.4.1",
    "@playwright/test": "^1.42.1",
    "@trivago/prettier-plugin-sort-imports": "^4.3.0",
    "@types/node": "^20.12.6",
    "@typescript-eslint/eslint-plugin": "^7.7.0",
    "@typescript-eslint/parser": "^7.7.0",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-playwright": "^1.6.0",
    "eslint-plugin-prettier": "^5.1.3"
  },
  "dependencies": {
    "dotenv": "^16.4.5"
  }
}
